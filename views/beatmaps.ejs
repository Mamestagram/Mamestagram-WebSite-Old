    <script nonce="<%= cspNonce %>">
        function process() {
            var $td = $(".ranking .wrap .flex .selection .special .ts");
            var $mods = $(".ranking .wrap .mods-selection li");
            let link = window.location.href, modNum = 0;
            const modsList = [
                "nf", // 0 : NoFail
                "ez", // 1 : Eazy
                "ts", // 2 : TouchScreen
                "hd", // 3 : Hidden
                "hr", // 4 : HardRock
                "sd", // 5 : SaddenDeath
                "dt", // 6 : DoubleTime
                "rx", // 7 : Relax
                "ht", // 8 : HalfTime
                "nc", // 9 : NightCore
                "fl", // 10 : FlashLight
                "at", // 11 : AutoPlay
                "so", // 12 : SpinOut
                "ap", // 13 : Autopilot
                "pf", // 14 : Perfect
                "k4", // 15 : Key4
                "k5", // 16 : Key5
                "k6", // 17 : Key6
                "k7", // 18 : Key7
                "k8", // 19 : Key8
                "fi", // 20 : FadeIn
                "rd", // 21 : Random
                "cm", // 22 : Cinema
                "tp", // 23 : TargetPoint
                "k9", // 24 : Key9
                "kc", // 25 : KeyCoop
                "k1", // 26 : Key1
                "k3", // 27 : Key3
                "k2", // 28 : Key2
                "v2", // 29 : ScoreV2
                "mr", // 30 : Mirror
            ];

            let notSelected = true;
            if ($td.hasClass("selected")) {
                notSelected = false;
                modNum += 1 << 2;
            }
            for (let i = 0; i < $mods.length; i++) {
                if ($mods.eq(i).children("button").hasClass("selected")) {
                    if ($mods.eq(i).children("button").hasClass("selected")) {
                        notSelected = false;
                        if ($mods.eq(i).attr("class") === "nm") {
                            break;
                        }
                    }
                    modNum += 1 << modsList.indexOf($mods.eq(i).attr("class"));
                    modNum += $mods.eq(i).attr("class") === "nc" ? 1 << modList.indexOf("dt") : 0;
                }
            }
            if (notSelected) {
                link = link.includes("&mods=<%= mods %>") ? link.replace("&mods=<%= mods %>", "&mods=none") : `${link}&mods=none`;
            }
            else {
                link = link.includes("&mods=<%= mods %>") ? link.replace("&mods=<%= mods %>", `&mods=${modNum}`) : `${link}&mods=${modNum}`;
            }
            ajaxUpdate(link, document.getElementById("ajaxreload"));
        }

        function ajaxUpdate (link, element) {
            const ajax = new XMLHttpRequest();
            ajax.open("GET", link, true);
            ajax.onload = function () {
                element.innerHTML = ajax.responseText;
            }
            ajax.send(null);
        }

        function jump() {
            setTimeout(process, 1);
        }
    </script>
    </body>
</html>