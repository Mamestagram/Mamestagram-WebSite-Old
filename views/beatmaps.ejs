<!DOCTYPE html>
<html lang="ja">
    <head>
        <%
        const date = new Date().toLocaleString("ja-JP", {
            year: "numeric",
            month: "2-digit",
            day: "2-digit",
            hour: "2-digit",
            minute: "2-digit",
            second: "2-digit"
        }).replaceAll("/", "").replaceAll(" ", "").replaceAll(":", "");
        %>
        <title><%= mapdetails.artist %> - <%= mapdetails.title %> | Mamestagram</title>
        <meta name="robots" content="noindex">
        <%- include("link") %>
        <link rel="stylesheet" href="/css/beatmaps.css?<%= date %>" />
        <style>
            body::-webkit-scrollbar-track {
                background-color: #fdfdfd;
            }
            body::-webkit-scrollbar-thumb {
                background-color: #2e3538;
            }

            header {
                background-color: transparent;
            }

            header .pc .beatmaps {
                color: #fdfdfd;
            }

            .animated {
                background-color: #3986ac;
            }

            .animated .pc li:hover {
                background-color: #66ccff;
            }

            .animated .pc .beatmaps {
                border-bottom: 3px solid #65cafd;
            }

            @media (max-width: 1232px) {
                header {
                    background-color: #3986ac;
                }
            }

            .mobile {
                background-color: #22282a;
            }

            .mobile .beatmaps {
                background-color: rgba(102, 204, 255, 0.3);
                color: #fdfdfd;
            }

            .mobile li:hover {
                background-color: rgba(102, 204, 255, 0.3);
            }

            .mobile .register, .mobile .login, .mobile .avatar {
                background-color: #29363d;
                color: #79bdd1;
            }
            .mobile .register:hover, .mobile .login:hover, .mobile .avatar:hover {
                background-color: #29363d;
            }

            .link-form a, .link-form button {
                background-color: #3986ac;
            }
            .link-form a:hover, .link-form button {
                background-color: #4497c0;
            }

            .search-form .searchbox {
                background-color: #394246;
            }

            .search-form .searchbox .txtbox {
                border-bottom: 2px solid #5c6970;
            }
        </style>
        <%
        const getMode = (modeNum) => {
            switch (modeNum) {
                case 0:
                    return "osu";
                case 1:
                    return "taiko";
                case 2:
                    return "fruits";
                case 3:
                    return "mania";
            }
        }
        const getParamsMode = (modeNum) => {
            switch (modeNum) {
                case 0: return "std";
                case 1: return "taiko";
                case 2: return "ctb";
                case 3: return "mania";
                case 4: return "stdrx";
                case 5: return "taikorx";
                case 6: return "ctbrx";
                case 8: return "stdap";
            }
        }
        const getColor = (diff) => {
            const color = [
                "#4291fd", "#4396fd", "#449bfd", "#46a0fd", // 0.0 ~ 0.5
                "#47a4fd", "#48a9fd", "#4aaffd", "#4bb3fd", // 0.5 ~ 1.0
                "#4cb8fd", "#4dbcfd", "#4ec3f9", "#4ecdf2", // 1.0 ~ 1.5
                "#4ed8eb", "#4ee3e4", "#4eeedc", "#4ef8d6", // 1.5 ~ 2.0
                "#53fdc0", "#5efda2", "#69fd82", "#74fd5f", // 2.0 ~ 2.5
                "#82fc4f", "#97f951", "#abf753", "#bef455", // 2.5 ~ 3.0
                "#d0f257", "#e2f059", "#f4ec5b", "#f5dd5d", // 3.0 ~ 3.5
                "#f6cf5e", "#f8be60", "#f9af62", "#faa063", // 3.5 ~ 4.0
                "#fc8f65", "#fd8167", "#fd7868", "#fd6f69", // 4.0 ~ 4.5
                "#fd676a", "#fd5d6c", "#fd536d", "#fa4d70", // 4.5 ~ 5.0
                "#f24b7b", "#eb4a85", "#e2498f", "#da4799", // 5.0 ~ 5.5
                "#d346a3", "#cc45ac", "#c444b6", "#b548bd", // 5.5 ~ 6.0
                "#a84cc0", "#9b50c6", "#8f54cb", "#8059d0", // 6.0 ~ 6.5
                "#735dd6", "#6561db", "#5a58d2", "#524fc9", // 6.5 ~ 7.0
                "#4745be", "#3f3db6", "#3532ab", "#2a28a0", // 7.0 ~ 7.5
                "#221f97", "#19168e", "#16137f", "#131172", // 7.5 ~ 8.0
                "#110f64", "#0f0d55", "#0c0b46", "#0a0839", // 8.0 ~ 8.5
                "#07062b", "#05051f", "#030211", "#010104", // 8.5 ~ 9.0
                "#000000" // 9.0 ~
            ]
            let index = Math.floor(diff / 0.125);
            if (index > 72) {
                index = 72;
            }
            return color[index];
        }
        const getMods = (modNum) => {
            let mod = new Array(0);
            const mods = [
                "nf", // 0 : NoFail
                "ez", // 1 : Eazy
                "ts", // 2 : TouchScreen
                "hd", // 3 : Hidden
                "hr", // 4 : HardRock
                "sd", // 5 : SaddenDeath
                "dt", // 6 : DoubleTime
                "rx", // 7 : Relax
                "ht", // 8 : HalfTime
                "nc", // 9 : NightCore
                "fl", // 10 : FlashLight
                "at", // 11 : AutoPlay
                "so", // 12 : SpinOut
                "ap", // 13 : Autopilot
                "pf", // 14 : Perfect
                "k4", // 15 : Key4
                "k5", // 16 : Key5
                "k6", // 17 : Key6
                "k7", // 18 : Key7
                "k8", // 19 : Key8
                "fi", // 20 : FadeIn
                "rd", // 21 : Random
                "cm", // 22 : Cinema
                "tp", // 23 : TargetPoint
                "k9", // 24 : Key9
                "kc", // 25 : KeyCoop
                "k1", // 26 : Key1
                "k3", // 27 : Key3
                "k2", // 28 : Key2
                "v2", // 29 : ScoreV2
                "mr"  // 30 : Mirror
            ];
            for (let i = 30; i >= 0; i--) {
                if (modNum >= 1 << i) {
                    mod.push(mods[i]);
                    modNum -= 1 << i;
                    modNum -= mods[i] === "nc" ? 1 << mods.indexOf("dt") : 0;
                }
            }
            mod = sortMods(mod);
            return mod;
        }
        const sortMods = (mods) => {
            const order = ["ts", "rx", "ap", "k4", "k5", "k6", "k7", "k8", "k9", "nf", "ez", "ht", "hd", "hr", "dt", "nc", "sd", "pf", "fl", "fi", "rd", "mr", "so", "v2"];
            let rst = new Array(0);
            for (let i = 0; i < order.length; i++) {
                for (let mod of mods) {
                    if (mod === order[i]) {
                        rst.push(mod);
                        break;
                    }
                }
            }
            return rst;
        }
        const getModeName = (modeNum) => {
            switch (modeNum) {
                case 0: return "std";
                case 1: return "taiko";
                case 2: return "ctb";
                case 3: return "mania";
                case 4: return "stdrx";
                case 5: return "taikorx";
                case 6: return "ctbrx";
                case 8: return "stdap";
                default: return "std";
            }
        }
        const getTime = (time) => {
            let res = Math.floor(time / 60).toLocaleString() + ":";
            if (time % 60 >= 10) {
                res += time % 60;
            }
            else {
                res += "0" + time % 60;
            }
            return res;
        }
        const getPlayTime = (y, m, w, d, h, f) => {
            if (f === "brev") {
                if (y > 0) {
                    return `${y.toLocaleString()}y`;
                }
                else if (m > 0) {
                    return `${m}m`;
                }
                else if (w > 0) {
                    return `${w}w`;
                }
                else if (d > 0) {
                    return `${d}d`;
                }
                else if (h > 0) {
                    return `${h}h`;
                }
                else {
                    return "now";
                }
            }
            else if (f === "form") {
                if (y > 0) {
                    return `${y.toLocaleString()} year(s) ago`;
                }
                else if (m > 0) {
                    return `${m} month(s) ago`;
                }
                else if (w > 0) {
                    return `${w} week(s) ago`;
                }
                else if (d > 0) {
                    return `${d} day(s) ago`;
                }
                else if (h > 0) {
                    return `${h} hour(s) ago`;
                }
                else {
                    return "now";
                }
            }
        }
        const getRate = (value) => {
            return Math.min(100, value / 10 * 100);
        }
        %>
    </head>
    <body>
    <%- include("header") %>
    <main>
        <!-- map info -->
        <div class="map-info">
            <h1 class="title"><i class="fa-brands fa-itunes-note"></i>Beatmap info</h1>
            <div class="background">
                <img src="https://assets.ppy.sh/beatmaps/<%= setid %>/covers/cover.jpg?<%= date %>" alt="" />
                <div class="contents">
                    <div class="wrap flex-mobile">
                        <div class="diff">
                            <% mapinfo.forEach((map) => { %>
                                <a href="/beatmaps/<%= map.set_id %>/<%= map.id %>" title="â˜…<%= map.diff.toFixed(2) %> <%= map.version %>"
                                    <% if (map.id === Number(mapid)) { %>
                                        style="
                                        background-color: rgba(31, 41, 46, 0.5);
                                        border: 2px solid #b7d1de;"
                                    <% } %>>
                                    <i class="mode-icon mode-<%= getMode(map.mode) %>" style="color: <%= getColor(map.diff) %>;
                                    <% if (map.id === Number(mapid)) { %>
                                        opacity: 1;
                                    <% } %>">
                                    </i>
                                </a>
                            <% }); %>
                        </div>
                        <div class="map-status" style="
                            <% if (mapdetails.status === 2 || mapdetails.status === 3) { %>
                                background-color: #b3ff66;
                                color: #3d4847;
                            <% } else { %>
                                background-color: #fd65aa;
                                color: #394246;
                            <% } %>">
                            <% if (mapdetails.status === 2 || mapdetails.status === 3) { %>
                                RANKED
                            <% } else { %>
                                LOVED
                            <% } %>
                        </div>
                    </div>
                    <div class="wrap">
                        <div class="box">
                            <div class="diff-details">
                                <span class="diffname"><%= mapdetails.version %></span>
                                <span class="diffvalue">â˜…<%= mapdetails.diff.toFixed(2) %></span>
                                <span class="playcount">
                                    <i class="fa-solid fa-circle-play" style="padding: 0 5px;"></i><%= mapdetails.plays.toLocaleString() %>
                                </span>
                                <span class="mapper">
                                    mapped by
                                    <a href="https://osu.ppy.sh/users/<%= mapdetails.creator %>" target="_blank" rel="noopener noreferrer">
                                        <%= mapdetails.creator %>
                                    </a>
                                </span>
                                <div class="map-name">
                                    <span class="title"><%= mapdetails.title %></span>
                                        <a href="https://osu.ppy.sh/beatmapsets/<%= setid %>#<%= getMode(mapdetails.mode) %>/<%= mapid %>" title="Bancho link" target="_blank" rel="noopener noreferrer">
                                            <i class="fa-solid fa-arrow-up-right-from-square"></i>
                                        </a><br>
                                    <span class="artist"><%= mapdetails.artist %></span>
                                </div>
                                <div class="link">
                                    <audio id="preview">
                                        <source src="https://b.ppy.sh/preview/<%= setid %>.mp3" type="audio/mp3">
                                    </audio>
                                    <button id="play-audio" class="pause">
                                        <i class="fa-solid fa-play"></i>
                                    </button>
                                    <a href="https://api.nerinyan.moe/d/<%= setid %>" class="download">
                                        Download<i class="fa-solid fa-download"></i>
                                    </a>
                                    <a href="/direct/<%= setid %>">
                                        Direct<i class="fa-solid fa-download"></i>
                                    </a>
                                </div>
                            </div>
                        </div>
                        <div class="map-details">
                            <div class="main">
                                <span class="length" title="Total Length"><i class="fa-solid fa-clock-rotate-left"></i><%= getTime(mapdetails.total_length) %></span>
                                <span class="bpm" title="BPM"><i class="fa-solid fa-truck-fast"></i><%= mapdetails.bpm %></span>
                                <span class="circles" title="Max Combo"><i class="fa-regular fa-circle-dot"></i><%= mapdetails.max_combo %></span>
                            </div>
                            <div class="sub">
                                <div class="cs">
                                    <p class="title">Circle Size</p>
                                    <div class="bar">
                                        <span class="color" style="width: <%= getRate(mapdetails.cs) %>%;"></span>
                                    </div>
                                    <p class="value"><%= mapdetails.cs.toFixed(1) %></p>
                                </div>
                                <div class="hp">
                                    <p class="title">HP Drain</p>
                                    <div class="bar">
                                        <span class="color" style="width: <%= getRate(mapdetails.hp) %>%;"></span>
                                    </div>
                                    <p class="value"><%= mapdetails.hp.toFixed(1) %></p>
                                </div>
                                <div class="od">
                                    <p class="title">Accuracy</p>
                                    <div class="bar">
                                        <span class="color" style="width: <%= getRate(mapdetails.od) %>%;"></span>
                                    </div>
                                    <p class="value"><%= mapdetails.od.toFixed(1) %></p>
                                </div>
                                <div class="ar">
                                    <p class="title">Approach Rate</p>
                                    <div class="bar">
                                        <span class="color" style="width: <%= getRate(mapdetails.ar) %>%;"></span>
                                    </div>
                                    <p class="value"><%= mapdetails.ar.toFixed(1) %></p>
                                </div>
                                <div class="sr">
                                    <p class="title">Star Rating</p>
                                    <div class="bar">
                                        <span class="color" style="width: <%= getRate(mapdetails.diff) %>%;"></span>
                                    </div>
                                    <p class="value"><%= mapdetails.diff.toFixed(2) %></p>
                                </div>
                                <div class="ps" title="<%= mapdetails.passes %> / <%= mapdetails.plays %>">
                                    <p class="title">Success Rate</p>
                                    <div class="bar">
                                        <span class="color" style="width: <%= mapdetails.rate %>%;"></span>
                                    </div>
                                    <div class="frame">
                                        <span class="space" style="padding-left: <%= mapdetails.rate %>%"></span>
                                        <p class="value">
                                            <% if (mapdetails.rate === null) { %>
                                                0.0%
                                            <% } else { %>
                                                <%= mapdetails.rate.toFixed(1) %>%
                                            <% } %>
                                        </p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="ranking">
            <div class="wrap">
                <div class="flex">
                    <h1><i class="fa-solid fa-trophy"></i>Score Ranking</h1>
                    <div class="selection">
                        <div class="special">
                            <% if (modeNum % 4 === 0) { %>
                                <button class="ts" title="Touch Device" onclick="jump()"
                                <% if (getMods(mods).includes("ts")) { %>
                                    selected
                                <% } %>><i class="fa-solid fa-tv"></i></button>
                            <% } %>
                            <% if (modeNum !== 3) { %>
                                <button class="menu-title">
                                    <% if (modeNum >= 0 && modeNum < 4) { %>
                                        Classic
                                    <% } else if (modeNum < 8) { %>
                                        Relax
                                    <% } else { %>
                                        Auto Pilot
                                    <% } %>
                                    <i class="fa-solid fa-chevron-down arrow down"></i>
                                </button>
                                <ul class="dropdown">
                                    <!-- classic -->
                                    <li class="classic">
                                        <a
                                        <% if (modeNum % 4 === 0) { %>
                                            href="/beatmaps/<%= mapdetails.set_id %>&std/<%= mapdetails.id %>"
                                        <% } else if (modeNum % 4 === 1) { %>
                                            href="/beatmaps/<%= mapdetails.set_id %>&taiko/<%= mapdetails.id %>"
                                        <% } else if (modeNum % 4 === 2) { %>
                                            href="/beatmaps/<%= mapdetails.set_id %>&ctb/<%= mapdetails.id %>"
                                        <% } else { %>
                                            href="/beatmaps/<%= mapdetails.set_id %>&mania/<%= mapdetails.id %>"
                                        <% } %>>Classic</a>
                                    </li>
                                    <!-- relax -->
                                    <% if (modeNum !== 3) { %>
                                        <li class="relax">
                                            <a
                                            <% if (modeNum % 4 === 0) { %>
                                                href="/beatmaps/<%= mapdetails.set_id %>&stdrx/<%= mapdetails.id %>"
                                            <% } else if (modeNum % 4 === 1) { %>
                                                href="/beatmaps/<%= mapdetails.set_id %>&taikorx/<%= mapdetails.id %>"
                                            <% } else if (modeNum % 4 === 2) { %>
                                                href="/beatmaps/<%= mapdetails.set_id %>&ctbrx/<%= mapdetails.id %>"
                                            <% } %>>Relax</a>
                                        </li>
                                    <% } %>
                                    <% if (modeNum % 4 === 0) { %>
                                        <!-- auto pilot -->
                                        <li class="auto-pilot">
                                            <a href="/beatmaps/<%= mapdetails.set_id %>&stdap/<%= mapdetails.id %>">Auto Pilot</a>
                                        </li>
                                    <% } %>
                                </ul>
                            <% } %>
                        </div>
                        <% if (mapdetails.mode === 0) { %>
                            <ul class="convert">
                                <li class="std"
                                <% if (modeNum % 4 === 0) { %>
                                    style="filter: invert(0%); opacity: 1"
                                <% } %>>
                                    <a
                                    <% if (modeNum >= 0 && modeNum < 4) { %>
                                        href="/beatmaps/<%= mapdetails.set_id %>&std/<%= mapdetails.id %>"
                                    <% } else if (modeNum < 7) { %>
                                        href="/beatmaps/<%= mapdetails.set_id %>&stdrx/<%= mapdetails.id %>"
                                    <% } else { %>
                                        href="/beatmaps/<%= mapdetails.set_id %>&stdap/<%= mapdetails.id %>"
                                    <% } %>><i class="mode-icon mode-osu"></i></a>
                                </li>
                                <% if (modeNum !== 8) { %>
                                    <li class="taiko"
                                    <% if (modeNum % 4 === 1) { %>
                                        style="filter: invert(0%); opacity: 1"
                                    <% } %>>
                                        <a
                                        <% if (modeNum >= 0 && modeNum < 4) { %>
                                            href="/beatmaps/<%= mapdetails.set_id %>&taiko/<%= mapdetails.id %>"
                                        <% } else { %>
                                            href="/beatmaps/<%= mapdetails.set_id %>&taikorx/<%= mapdetails.id %>"
                                        <% } %>><i class="mode-icon mode-taiko"></i></a>
                                    </li>
                                    <li class="catch"
                                    <% if (modeNum % 4 === 2) { %>
                                        style="filter: invert(0%); opacity: 1"
                                    <% } %>>
                                        <a
                                        <% if (modeNum >= 0 && modeNum < 4) { %>
                                            href="/beatmaps/<%= mapdetails.set_id %>&ctb/<%= mapdetails.id %>"
                                        <% } else { %>
                                            href="/beatmaps/<%= mapdetails.set_id %>&ctbrx/<%= mapdetails.id %>"
                                        <% } %>><i class="mode-icon mode-fruits"></i></a>
                                    </li>
                                <% } %>
                                <% if (modeNum >= 0 && modeNum < 4) { %>
                                    <li class="mania"
                                    <% if (modeNum === 3) { %>
                                        style="filter: invert(0%); opacity: 1"
                                    <% } %>>
                                        <a href="/beatmaps/<%= mapdetails.set_id %>&mania/<%= mapdetails.id %>">
                                            <i class="mode-icon mode-mania"></i>
                                        </a>
                                    </li>
                                <% } %>
                            </ul>
                        <% } %>
                    </div>
                </div>
                <div class="mods-selection">
                    <ul>
                        <li class="nm">
                            <button title="No Mod" onclick="jump()"
                            <% if (Number(mods) === 0) { %>
                                class="selected"
                            <% } %>><i class="fa-solid fa-xmark"></i></button>
                        </li>
                        <li class="ez">
                            <button title="Eazy" onclick="jump()"
                            <% if (getMods(mods).includes("ez")) { %>
                                class="selected"
                            <% } %>><img src="/image/mods/ez.png?<%= date %>" alt="" /></button>
                        </li>
                        <li class="nf">
                            <button title="No Fail" onclick="jump()"
                            <% if (getMods(mods).includes("nf")) { %>
                                class="selected"
                            <% } %>><img src="/image/mods/nf.png?<%= date %>" alt="" /></button>
                        </li>
                        <li class="ht">
                            <button title="Half Time" onclick="jump()"
                            <% if (getMods(mods).includes("ht")) { %>
                                class="selected"
                            <% } %>><img src="/image/mods/ht.png?<%= date %>" alt="" /></button>
                        </li>
                        <li class="hr">
                            <button title="Hard Rock" onclick="jump()"
                            <% if (getMods(mods).includes("hr")) { %>
                                class="selected"
                            <% } %>><img src="/image/mods/hr.png?<%= date %>" alt="" /></button>
                        </li>
                        <li class="sd">
                            <button title="Sudden Death" onclick="jump()"
                            <% if (getMods(mods).includes("sd")) { %>
                                class="selected"
                            <% } %>><img src="/image/mods/sd.png?<%= date %>" alt="" /></button>
                        </li>
                        <li class="pf">
                            <button title="Perfect" onclick="jump()"
                            <% if (getMods(mods).includes("pf")) { %>
                                class="selected"
                            <% } %>><img src="/image/mods/pf.png?<%= date %>" alt="" /></button>
                        </li>
                        <li class="dt">
                            <button title="Double Time" onclick="jump()"
                            <% if (getMods(mods).includes("dt")) { %>
                                class="selected"
                            <% } %>><img src="/image/mods/dt.png?<%= date %>" alt="" /></button>
                        </li>
                        <li class="nc">
                            <button title="Nightcore" onclick="jump()"
                            <% if (getMods(mods).includes("nc")) { %>
                                class="selected"
                            <% } %>><img src="/image/mods/nc.png?<%= date %>" alt="" /></button>
                        </li>
                        <% if (modeNum === 3) { %>
                            <li class="fi">
                                <button title="Fade In" onclick="jump()"
                                <% if (getMods(mods).includes("fi")) { %>
                                    class="selected"
                                <% } %>><img src="/image/mods/fi.png?<%= date %>" alt="" /></button>
                            </li>
                        <% } %>
                        <li class="hd">
                            <button title="Hidden" onclick="jump()"
                            <% if (getMods(mods).includes("hd")) { %>
                                class="selected"
                            <% } %>><img src="/image/mods/hd.png?<%= date %>" alt="" /></button>
                        </li>
                        <li class="fl">
                            <button title="Flashlight" onclick="jump()"
                            <% if (getMods(mods).includes("fl")) { %>
                                class="selected"
                            <% } %>><img src="/image/mods/fl.png?<%= date %>" alt="" /></button>
                        </li>
                        <% if (modeNum === 0) { %>
                            <li class="so">
                                <button title="Spun Out" onclick="jump()"
                                <% if (getMods(mods).includes("so")) { %>
                                    class="selected"
                                <% } %>><img src="/image/mods/so.png?<%= date %>" alt="" /></button>
                            </li>
                        <% } else if (modeNum === 3) { %>
                            <% if (mapdetails.mode === 0) { %>
                                <% for (let i = 4; i <= 9; i++) { %>
                                    <li class="k<%= i %>">
                                        <button title="key <%= i %>keys" onclick="jump()"
                                        <% if (getMods(mods).includes(`k${i}`)) { %>
                                            class="selected"
                                        <% } %>><img src="/image/mods/k<%= i %>.png?<%= date %>" alt="" /></button>
                                    </li>
                                <% } %>
                            <% } %>
                            <li class="rd">
                                <button title="Random" onclick="jump()"
                                <% if (getMods(mods).includes("rd")) { %>
                                    class="selected"
                                <% } %>><img src="/image/mods/rd.png?<%= date %>" alt="" /></button>
                            </li>
