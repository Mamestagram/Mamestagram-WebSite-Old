                        <% if (data.n100 === 0) { %>
                            style="color: #7e8487;"
                        <% } %>><%= data.n100.toLocaleString() %></td>
                        <td class="n-amt"
                        <% if (data.n50 === 0) { %>
                            style="color: #7e8487;"
                        <% } %>><%= data.n50.toLocaleString() %>/<%= (data.n50 + data.nkatu).toLocaleString() %></td>
                    <% } else if (mapdetails.mode % 4 === 3 || modeNum % 4 === 3) { %>
                        <td class="n-amt"
                        <% if (data.ngeki === 0) { %>
                            style="color: #7e8487;"
                        <% } %>><%= data.ngeki.toLocaleString() %></td>
                        <td class="n-amt"
                        <% if (data.n300 === 0) { %>
                            style="color: #7e8487;"
                        <% } %>><%= data.n300.toLocaleString() %></td>
                        <td class="n-amt"
                        <% if (data.nkatu === 0) { %>
                            style="color: #7e8487;"
                        <% } %>><%= data.nkatu.toLocaleString() %></td>
                        <td class="n-amt"
                        <% if (data.n100 === 0) { %>
                            style="color: #7e8487;"
                        <% } %>><%= data.n100.toLocaleString() %></td>
                        <td class="n-amt"
                        <% if (data.n50 === 0) { %>
                            style="color: #7e8487;"
                        <% } %>><%= data.n50.toLocaleString() %></td>
                    <% } %>
                    <td class="n-amt"
                    <% if (data.nmiss === 0) { %>
                        style="color: #7e8487;"
                    <% } %>><%= data.nmiss.toLocaleString() %></td>
                    <td><%= Math.round(data.pp).toLocaleString() %></td>
                    <td><%= getPlayTime(data.year, data.month, data.week, data.day, data.hour, "brev") %></td>
                    <td class="mods">
                        <div class="image">
                            <% getMods(data.mods).forEach((mod) => { %>
                                <img title="<%= mod %>" class="mod" src="/image/mods/<%= mod %>.png" alt="">
                            <% }); %>
                        </div>
                    </td>
                </tr>
                <% if (++n == 50) { break; } %>
            <% } %>
        </table>
    </div>
<% } %>